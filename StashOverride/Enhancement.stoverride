name: Enhance stash
desc: Block QUIC,force http engine to work when tunnel mode on.

rules:
  - SCRIPT,quic,REJECT,no-track

script:
  shortcuts: # 可以在 rule 中引用
    quic: network == 'udp' and dst_port == 443 # 匹配 QUIC 协议

http:
  # 强制使用 Stash 引擎以 HTTP 协议处理 TCP 连接
  # 捕获后的连接可以使用高级功能，例如重写和脚本
  force-http-engine:
    - '*:80'
    - '*:4480' # BiliBili CDN
    - '*:9102' # BiliBili CDN